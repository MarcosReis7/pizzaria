program Pizzaria;

uses
  Vcl.Forms,
  Tela_2 in 'Tela_2.pas' {frm_tela2},
  Tela_3 in 'Tela_3.pas' {frm_tela3},
  Tela_4 in 'Tela_4.pas' {Tela_calculadora};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TTela_calculadora, Tela_calculadora);
  Application.CreateForm(Tfrm_tela2, frm_tela2);
  Application.CreateForm(Tfrm_tela3, frm_tela3);
  Application.CreateForm(TTela_calculadora, Tela_calculadora);
  Application.Run;
end.
